	LET $POSITION_CONV_DT		= ''
	LET #SCHEDULED_WEEKLY_HRS	= 0
	LET $PAYGROUP_NAME		= ''
	LET $PAYGROUP_KEY		= ''
	LET $PAYGROUP_COUNTRY		= ''
	LET $PAYGROUP_ID		= ''
	LET $PAYGROUP_DESCR		= ''
	LET $PAYGROUP			= ''
	LET $PROBATION_START_DT		= ''
	LET $PROBATION_END_DT		= ''
	LET $BENEFIT_SERVICE_DT		= ''
	LET $COMPANY_SERVICE_DT		= ''
	LET $END_EMPLOYMENT_DT		= ''
	LET $RETIREMENT_DT		= ''
	LET $SENIORITY_DT		= ''
	LET $MANAGER_POSITION		= ''
	LET $MANAGER_ID			= ''
	LET $MANAGER_NAME		= ''
	LET $SUPERVISOR_ID		= ''
	LET #DISTRIBUTION_AMT		= 0
	LET #DISTRIBUTION_PERCENT	= 0
	LET $ACCOUNT_NAME		= ''
	LET $ACCOUNT_EC_ID		= ''
	LET #ACCOUNT_ID			= 0
