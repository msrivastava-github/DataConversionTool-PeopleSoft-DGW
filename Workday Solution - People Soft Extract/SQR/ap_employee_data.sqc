!*********************************
Begin-Procedure Init_Variables_Emp
!*********************************
LET $COMPANY_INSIDER_TYPE_NAME		= ''
LET $ANNUAL_WORK_PERIOD_NAME		= ''
LET $DISBURSEMENT_PLAN_PERIOD_NAME	= ''


Let $where_jobcode	= 'JCT.EFFDT = (SELECT MAX(JCT_ED.EFFDT) FROM PS_JOBCODE_TBL JCT_ED WHERE JCT.SETID = JCT_ED.SETID AND JCT.JOBCODE=JCT_ED.JOBCODE AND JCT_ED.EFFDT <= '''||$SYSDATE||''') AND JCT.EFF_STATUS =''A'''
load-lookup
name=JOBCODE_TBL
table='PS_JOBCODE_TBL JCT'
key=SETID||JOBCODE
return_value=DESCR||';'||STD_HRS_FREQUENCY
where=$where_jobcode
QUIET


load-lookup
name=NID_TYPE
table=PS_NID_TYPE1_VW
key=COUNTRY||','||NATIONAL_ID_TYPE
return_value=DESCR
QUIET

Let $where_clause	= 'B.EFFDT = (SELECT MAX(B_ED.EFFDT) FROM PS_VISA_PERMIT_TBL B_ED WHERE B.COUNTRY = B_ED.COUNTRY AND B.VISA_PERMIT_TYPE = B_ED.VISA_PERMIT_TYPE AND B_ED.EFFDT <='||''''||$SYSDATE||''''||') AND B.EFF_STATUS =' ||''''||'A'||''''
load-lookup
name=VISA_TBL
table='PS_VISA_PERMIT_TBL B'
key=COUNTRY||','||VISA_PERMIT_TYPE
return_value=DESCR
where=$where_clause
QUIET

Let $where_clause1	= 'DIA.EFFDT=(SELECT MAX(DIA1.EFFDT) FROM PS_DIAGNOSIS_TBL DIA1 WHERE DIA.SETID=DIA1.SETID AND DIA.DIAGNOSIS_CD=DIA1.DIAGNOSIS_CD AND DIA1.EFFDT<='||''''||$SYSDATE||''''||') AND EFF_STATUS = '||''''||'A'||''''
load-lookup
name=DIAGNOSIS_TBL
table='PS_DIAGNOSIS_TBL DIA'
key=SETID||','||DIAGNOSIS_CD
return_value=DESCR
where=$where_clause1
QUIET

load-lookup
name=CITIZENSHIP
table=PS_CITIZEN_STS_TBL
key=COUNTRY||','||CITIZENSHIP_STATUS
return_value=DESCR
QUIET

LET $where_clause1	= 'EFF_STATUS=''A'''
load-lookup
name=BANK_EC_TBL
table=PS_BANK_EC_TBL
key=COUNTRY_CD||BANK_CD
return_value=BANK_NM
where=$where_clause1
QUIET


LET $where_clause1	= 'EFF_STATUS=''A'''
load-lookup
name=BANK_BRANCH_TBL
table=PS_BANK_BRANCH_TBL
key=COUNTRY_CD||BANK_CD||BRANCH_EC_CD
return_value=DESCR||';'||BANK_ID_CODE
where=$where_clause1
QUIET

Let $where_clause1	= 'RLG.EFFDT=(SELECT MAX(RLG1.EFFDT) FROM PS_RELIGION_TBL RLG1 WHERE RLG.SETID=RLG1.SETID AND RLG.RELIGION_CD=RLG1.RELIGION_CD AND RLG1.EFFDT<='||''''||$SYSDATE||''''||') AND EFF_STATUS = ''A'''
load-lookup
name=RELIGION_TBL
table='PS_RELIGION_TBL RLG'
key=SETID||','||RELIGION_CD
return_value=DESCR
where=$where_clause1
QUIET

Let $PHONE_DEVICE_TYPE	= 'Landline'
Let $PASSPORT_TYPE_NAME	= ''
Let $PASSPORT_TYPE	= 'STANDARD'
Let $E_ADDR_TYPE	= 'E_ADDR_TYPE IN ('
Let $E_ADDR_TYPE	= $E_ADDR_TYPE|| '''' || 'HOME' || ''''||', ' ||''''|| 'BUSN' ||'''' || ')'		! Configure here what Email Address types we require. Possible types are DORM, BUSN, HOME, OTHR & CAMP.

!Address type(s) can be removed from below string if we don't want to extract particular Address Type(s)
Let $ADDRESS_TYPE	= 'AND ADDRESS_TYPE IN ('
Let $ADDRESS_TYPE	= $ADDRESS_TYPE|| '''' || 'HOME' || ''''||', ' ||''''|| 'MAIL' ||''''||', ' ||''''|| 'BUSN' ||'''' || ', '
Let $ADDRESS_TYPE	= $ADDRESS_TYPE|| '''' || 'CHK' || ''''||', ' ||''''|| 'DORM' ||''''||', ' ||''''|| 'LEGL' ||'''' || ', '
Let $ADDRESS_TYPE	= $ADDRESS_TYPE|| '''' || 'CAMP' || ''''||', ' ||''''|| 'OTH' ||''''||', ' ||''''|| 'BILL' ||'''' || ', '
Let $ADDRESS_TYPE	= $ADDRESS_TYPE|| '''' || 'OTH2' || ''''||', ' ||''''|| 'PERM' ||''''||', ' ||''''|| 'PREF' ||'''' ||', ' ||''''|| 'VTRN' ||'''' || ')'

!Below part of code will extract data from Compensation configuration page

BEGIN-SELECT
C1.EFFDT
FROM PS_AP_COMP_EFFDT C1
WHERE C1.EFFDT =
  (SELECT MAX(C1_ED.EFFDT)
  FROM PS_AP_COMP_EFFDT C1_ED
  WHERE C1_ED.EFFDT <= $SYSDATE
  )
AND EFF_STATUS='A'
END-SELECT

LET $ALLOWANCE_PLAN		= ''
LET $BONUS_PLAN			= ''
LET $COMMISSION_PLAN		= ''
LET $MERIT_PLAN			= ''

BEGIN-SELECT
C2.COMP_RATECD
AP_PLAN_TYPE
	EVALUATE &AP_PLAN_TYPE
	WHEN='A'
	
		LET $ALLOWANCE_PLAN	= $ALLOWANCE_PLAN || ', ' || ''''|| &C2.COMP_RATECD || ''''
	BREAK
	WHEN='B'
	
		LET $BONUS_PLAN		= $BONUS_PLAN || ', ' || ''''|| &C2.COMP_RATECD || ''''
	BREAK
	WHEN='C'
	
		LET $COMMISSION_PLAN	= $COMMISSION_PLAN || ', ' || ''''|| &C2.COMP_RATECD || ''''
	BREAK
	WHEN='M'
	
		LET $MERIT_PLAN		= $MERIT_PLAN || ', ' || ''''|| &C2.COMP_RATECD || ''''
	BREAK
	END-EVALUATE
FROM PS_AP_COMP_PLAN C2
WHERE C2.EFFDT  = &C1.EFFDT
END-SELECT

LET $ALLOWANCE_PLAN		= ltrim($ALLOWANCE_PLAN,',')
LET $BONUS_PLAN			= ltrim($BONUS_PLAN,',')
LET $COMMISSION_PLAN		= ltrim($COMMISSION_PLAN,',')
LET $MERIT_PLAN			= ltrim($MERIT_PLAN,',')

End-Procedure

