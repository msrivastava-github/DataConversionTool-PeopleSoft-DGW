	LET $POI_TYPE			= ''
	
	LET $EMPLID			= ltrim(rtrim(&JB1.EMPLID,' '),' ')
	LET #EMPL_RCD			= &JB1.EMPL_RCD
	LET $PER_ORG			= ltrim(rtrim(&JB1.PER_ORG,' '),' ')
