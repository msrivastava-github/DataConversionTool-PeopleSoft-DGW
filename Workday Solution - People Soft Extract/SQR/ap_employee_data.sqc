	LOOKUP BANK_EC_TBL $BANK_KEYS $BANK_NAME
	
	LET $BRANCH_EC_CD		= ltrim(rtrim(&BRANCH_EC_CD,' '),' ')
	LET $BRANCH_KEYS		= $COUNTRY_CD||$BANK_CD||$BRANCH_EC_CD
	LOOKUP BANK_BRANCH_TBL $BRANCH_KEYS $BRANCH_DETAILS
	unstring $BRANCH_DETAILS by ';' into $BRANCH_NAME $BIC
	
	LET $CHECK_DIGIT		= ltrim(rtrim(&CHECK_DIGIT,' '),' ')
	LET $INTL_BANK_ACCT_NBR		= ltrim(rtrim(&INTL_BANK_ACCT_NBR,' '),' ')
	
	do Get_Bank_Account_Distribution
	IF #ACCOUNT_ID <> 0
		EXIT-SELECT
	END-IF
FROM PS_PYE_BANKACCT BNK
WHERE EMPLID=$EMPLID
END-SELECT
END-PROCEDURE

