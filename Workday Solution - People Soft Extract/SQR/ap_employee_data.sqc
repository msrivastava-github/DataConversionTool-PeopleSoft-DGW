	LET $MANAGER_POSITION		= LTRIM(RTRIM(&JB1.REPORTS_TO,' '),' ')
	LET $SUPERVISOR_ID		= LTRIM(RTRIM(&JB1.SUPERVISOR_ID,' '),' ')
		
	IF $MANAGER_POSITION <> ''
		do Get_EmployeeID_ByPosition($MANAGER_POSITION,$MANAGER_ID)
		
		ELSE
		
		IF $SUPERVISOR_ID <> ''
			LET $MANAGER_ID		= $SUPERVISOR_ID
		END-IF
	END-IF
	
	DO Get_Name($MANAGER_ID,$MANAGER_NAME)
		
	LET $JOBCODE			= ltrim(rtrim(&JB1.JOBCODE,' '),' ')
	LET $JOB_CODE_KEY		= &JB1.SETID_JOBCODE||$JOBCODE
	LET $JOB_CODE_DETAIL		= ''
	LET $WORK_HOURS_PROFILE		= ''
	LET $WORK_HOURS_PROFILE_CODE	= ''
	LET $WORK_HOURS_PROFILE_DESCR	= ''
	LET $FREQ_DETAILS		= ''
	LET #FREQ_ANNUAL_FACTOR 	= 0
	LET $FREQ_ANNUAL_FACTOR 	= ''
	LET $USE_STD_HOURS		= ''

	lookup JOBCODE_TBL $JOB_CODE_KEY $JOB_CODE_DETAIL
	unstring $JOB_CODE_DETAIL by ';' into $JOB_PROFILE $WORK_HOURS_PROFILE_CODE

	lookup FREQUENCY_TBL $WORK_HOURS_PROFILE_CODE $FREQ_DETAILS
	
	LET #LOC_FREQ	= 0
	find ';' in $FREQ_DETAILS 0 #LOC_FREQ
	IF #LOC_FREQ	<> -1
		LET $WORK_HOURS_PROFILE_DESCR	= ltrim(rtrim(substrp($FREQ_DETAILS, 1, #LOC_FREQ),' '),' ')
	END-IF
	
	LET $TIME_TYPE			= ltrim(rtrim(&JB1.FULL_PART_TIME,' '),' ')
	Let $FIELDNAME			= 'FULL_PART_TIME'
	Let $FIELDVALUE			= ltrim(rtrim($TIME_TYPE,' '),' ')
	do Get_Xlat_Item
	Let $TIME_TYPE			= $XlatLongName
	LET #SCHEDULED_WEEKLY_HRS	= &JB1.STD_HOURS
	
	EVALUATE &JB1.PAY_SYSTEM_FLG
	WHEN = 'GP'
	
	LET $PAYGROUP_ID		= ltrim(rtrim(&JB1.GP_PAYGROUP,' '),' ')
	lookup GP_PYGRP $PAYGROUP_ID $PAYGROUP_NAME
	do Get_PayGroup_Country
	BREAK
	
	WHEN-Other
		LET $PAYGROUP_ID		= ltrim(rtrim(&JB1.PAYGROUP,' '),' ')
		LET $PAYGROUP_KEY		= $COMPANY_ID||$PAYGROUP_ID
		lookup PAYGROUP_TBL $PAYGROUP_KEY $PAYGROUP_DESCR
		unstring $PAYGROUP_DESCR by ';' into $PAYGROUP_NAME $PAYGROUP_COUNTRY
	BREAK
	
	END-EVALUATE
	
	LET $PROBATION_START_DT		= edit(&JB1.HIRE_DT,'YYYY-MM-DDT00:00:00.000')
	LET $END_EMPLOYMENT_DT		= edit(&JB1.EXPECTED_END_DATE,'YYYY-MM-DDT00:00:00.000')
	
	LET $ACTION= ltrim(rtrim(&JB1.ACTION,' '),' ')
	IF $ACTION = 'RET' OR $ACTION = 'RWP'
		LET $RETIREMENT_DT	= edit(&JB1.TERMINATION_DT,'YYYY-MM-DDT00:00:00.000')
	END-IF
	do Get_Severence_Date
	do Get_Bank_Account_Data
	
	LET $SETID_SAL			= ltrim(rtrim(&JB1.SETID_SALARY,' '),' ')
	
	LET $GRADE			= ltrim(rtrim(&JB1.GRADE,' '),' ')
	LET $SAL_PLAN			= ltrim(rtrim(&JB1.SAL_ADMIN_PLAN,' '),' ')
	LET $SAL_CURRENCY_CD		= ltrim(rtrim(&JB1.CURRENCY_CD,' '),' ')
	LET $SAL_PLAN_FREQ		= ltrim(rtrim(&JB1.COMP_FREQUENCY,' '),' ')
	LET $FREQ_DETAILS		= ''
	lookup FREQUENCY_TBL $SAL_PLAN_FREQ $FREQ_DETAILS
	
	LET #LOC_FREQ	= 0
	find ';' in $FREQ_DETAILS 0 #LOC_FREQ
	IF #LOC_FREQ	<> -1
		LET $SAL_PLAN_TYPE	= ltrim(rtrim(substrp($FREQ_DETAILS, 1, #LOC_FREQ),' '),' ')
	END-IF
	
	do Get_Comp_Details
	do Get_UserName
	IF &JB1.HR_STATUS = 'I'
		do Get_Termination_Details
	END-IF
	do write_Emp_Data
	
FROM PS_JOB JB1
WHERE JB1.EFFDT    =
  (SELECT MAX(JB1_ED.EFFDT)
  FROM PS_JOB JB1_ED
  WHERE JB1.EMPLID  = JB1_ED.EMPLID
  AND JB1.EMPL_RCD  = JB1_ED.EMPL_RCD
  AND JB1_ED.EFFDT <= $SYSDATE
  )
AND JB1.EFFSEQ =
  (SELECT MAX(JB1_ES.EFFSEQ)
  FROM PS_JOB JB1_ES
  WHERE JB1.EMPLID = JB1_ES.EMPLID
  AND JB1.EMPL_RCD = JB1_ES.EMPL_RCD
  AND JB1.EFFDT    = JB1_ES.EFFDT
  )
AND PER_ORG		= 'EMP'
AND JOB_INDICATOR	= 'P'				!Primary job only
ORDER BY JB1.EMPLID, JB1.EMPL_RCD, JB1.EFFSEQ
END-SELECT
END-PROCEDURE			! get_Emp_Data

!***********************************
Begin-Procedure Get_Hire_Reason
!***********************************
LET $ACTION		= ''
LET $REASON		= ''
BEGIN-SELECT
ACTION
ACTION_REASON
	LET $ACTION		= ltrim(rtrim(&ACTION,' '),' ')
	LET $REASON		= ltrim(rtrim(&ACTION_REASON,' '),' ')
	LET $HIRE_REASON_KEY	= $ACTION||$REASON
	lookup ACTN_REASON_TBL $HIRE_REASON_KEY $HIRE_REASON
FROM PS_JOB JOB1
WHERE JOB1.EFFDT    =
  (SELECT MAX(JOB1_ED.EFFDT)
  FROM PS_JOB JOB1_ED
  WHERE JOB1.EMPLID  = JOB1_ED.EMPLID
  AND JOB1.EMPL_RCD  = JOB1_ED.EMPL_RCD
  AND JOB1_ED.EFFDT  <= $SYSDATE
  AND JOB1.ACTION    = JOB1_ED.ACTION
  )
AND JOB1.EFFSEQ =
  (SELECT MAX(JOB1_ES.EFFSEQ)
  FROM PS_JOB JOB1_ES
  WHERE JOB1.EMPLID = JOB1_ES.EMPLID
  AND JOB1.EMPL_RCD = JOB1_ES.EMPL_RCD
  AND JOB1.EFFDT    = JOB1_ES.EFFDT
  AND JOB1.ACTION   = JOB1_ES.ACTION
  )
AND EMPLID = $EMPLID
AND EMPL_RCD = #EMPL_RCD
AND ACTION IN ('HIR','REH')
AND ACTION_REASON <> ' '
ORDER BY JOB1.EMPLID, JOB1.EMPL_RCD, JOB1.EFFSEQ
END-SELECT
END-PROCEDURE			! Get_Hire_Reason


!***********************************
Begin-Procedure Get_PayGroup_Country
!***********************************
BEGIN-SELECT
PYE.COUNTRY
	LET $PAYGROUP_COUNTRY	= ltrim(rtrim(&PYE.COUNTRY,' '),' ')
FROM PS_GP_PYGRP PYG, PS_GP_PYENT PYE
WHERE PYG.GP_PAYGROUP = $PAYGROUP_ID
AND PYE.PAY_ENTITY = PYG.PAY_ENTITY

END-SELECT
END-PROCEDURE			! Get_PayGroup_Country


!*********************************
Begin-Procedure Get_Severence_Date
!*********************************
LET $SEVERENCE_DT	= ''
BEGIN-SELECT
GVT_SEV_BEGIN_DT
	LET $SEVERENCE_DT	= edit(&GVT_SEV_BEGIN_DT,'YYYY-MM-DDT00:00:00.000')
FROM PS_GVT_UCFE SEV
WHERE SEV.GVT_REQUEST_DATE=
  (SELECT MAX(SEV_ED.GVT_REQUEST_DATE)
  FROM PS_GVT_UCFE SEV_ED
  WHERE SEV.EMPLID            =SEV_ED.EMPLID
  AND SEV_ED.GVT_REQUEST_DATE<=$SYSDATE
  )
  AND SEV.EMPLID=$EMPLID
  
END-SELECT
END-PROCEDURE		!	Get_Severence_Date

