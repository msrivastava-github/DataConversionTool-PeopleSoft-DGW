!**************************************
BEGIN-PROCEDURE Get_Termination_Details
!**************************************
BEGIN-SELECT
TER.TERMINATION_DT
TER.LAST_DATE_WORKED
TER.EXPECTED_RETURN_DT
TER.ACTION
TER.ACTION_REASON
	LET $TERMINATION_DT	= edit(&TER.TERMINATION_DT,'YYYY-MM-DDT00:00:00.000')
	LET $LAST_DATE_WORKED	= edit(&TER.LAST_DATE_WORKED,'YYYY-MM-DDT00:00:00.000')
	LET $EXPECTED_RETURN_DT	= edit(&TER.EXPECTED_RETURN_DT,'YYYY-MM-DDT00:00:00.000')
	LET $PRIMARY_TERM_ACTION= ltrim(rtrim(&TER.ACTION,' '),' ')
	LET $PRIMARY_TERM_REASON= ltrim(rtrim(&TER.ACTION_REASON,' '),' ')
	IF $PRIMARY_TERM_REASON <> ''
		LET $HIRE_REASON_KEY	= $PRIMARY_TERM_ACTION||$PRIMARY_TERM_REASON
		lookup ACTN_REASON_TBL $HIRE_REASON_KEY $PRIMARY_TERM_REASON
	END-IF
FROM PS_JOB TER
WHERE TER.EFFDT    =
  (SELECT MAX(TER_ED.EFFDT)
  FROM PS_JOB TER_ED
  WHERE TER.EMPLID  = TER_ED.EMPLID
  AND TER.EMPL_RCD  = TER_ED.EMPL_RCD
  AND TER.ACTION    = TER_ED.ACTION
  AND TER_ED.EFFDT <= $SYSDATE
  )
AND TER.EFFSEQ =
  (SELECT MAX(TER_ES.EFFSEQ)
  FROM PS_JOB TER_ES
  WHERE TER.EMPLID = TER_ES.EMPLID
  AND TER.EMPL_RCD = TER_ES.EMPL_RCD
  AND TER.EFFDT    = TER_ES.EFFDT
  AND TER.ACTION   = TER_ES.ACTION
  )
AND ACTION IN ('TWP','RET','RWP','TWB','TER', 'LOF')
AND EMPLID 		= $EMPLID
AND PER_ORG		= 'EMP'
AND JOB_INDICATOR	= 'P'
END-SELECT

END-PROCEDURE		!	Get_Termination_Details

