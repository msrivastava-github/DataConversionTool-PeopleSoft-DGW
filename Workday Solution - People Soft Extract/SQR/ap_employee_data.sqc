	LET $COMP_CURRENCY_CD		= ''
	LET $ALLOWANCE1_NM		= ''
	LET #ALLOWANCE1_PER		= 0
	LET #ALLOWANCE1_AMT		= 0
	LET $ALLOWANCE1_FREQ		= ''
	LET $ALLOWANCE1_CURR		= ''
	LET $ALLOWANCE2_NM		= ''
	LET #ALLOWANCE2_PER		= 0
