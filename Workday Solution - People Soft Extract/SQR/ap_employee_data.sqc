	unstring $DEPT_DETAILS by ';' into $DEPT_NAME $DEPT_MANAGER_ID $DEPT_MANAGER_POSITION

	LET $BU				= ltrim(rtrim(&JB1.BUSINESS_UNIT,' '),' ')
