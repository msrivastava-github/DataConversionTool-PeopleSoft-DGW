!*******************************
BEGIN-PROCEDURE Get_Comp_Details
!*******************************

LET $BASE_COMP_RATECD	= ''
LET #BONUS_TARGET	= 0
LET $BONUS_TYPE		= ''
LET $BONUS_FREQ_CD	= ''
LET $BONUS_FREQ		= ''
LET $BONUS_FREQ_DETAILS	= ''
LET $BONUS_CURRENCY	= ''
LET $BONUS_NM		= ''
LET #MERIT_TARGET	= 0
LET $MERIT_TYPE		= ''
LET $MERIT_FREQ_CD	= ''
LET $MERIT_FREQ		= ''
LET $MERIT_FREQ_DETAILS	= ''
LET $MERIT_CURRENCY	= ''
LET $MERIT_NM		= ''
LET #COMMISSION_TARGET	= 0
LET $COMMISSION_TYPE		= ''
LET $COMMISSION_FREQ_CD	= ''
LET $COMMISSION_FREQ		= ''
LET $COMMISSION_FREQ_DETAILS	= ''
LET $COMMISSION_CURRENCY	= ''
LET $COMMISSION_NM		= ''
LET #COUNT			= 1
BEGIN-SELECT
COMP.COMP_RATECD
COMP.COMPRATE
COMP.COMP_PCT
COMP.COMP_FREQUENCY
COMP.CURRENCY_CD
COMP.CONVERT_COMPRT
COMP.EFFSEQ
COMP.COMP_EFFSEQ
	LET $COMP_RATECD		= ltrim(rtrim(&COMP.COMP_RATECD,' '),' ')
	if $BASE_COMP_RATECD	= ''
		do Get_Base_Comp_Element
	end-if
	do Get_Allowance_Element
	if $BONUS_NM	= ''
		do Get_Bonus_Element
	end-if
	if $MERIT_NM	= ''
		do Get_Merit_Element
	end-if
	if $COMMISSION_NM	= ''
		do Get_Commission_Element
	end-if
FROM PS_COMPENSATION COMP
WHERE COMP.EMPLID    = $EMPLID
AND COMP.EMPL_RCD    = #EMPL_RCD
AND COMP.EFFDT       = &JB1.EFFDT
AND COMP.COMPRATE    <> 0
ORDER BY COMP.COMP_RATECD
END-SELECT

END-PROCEDURE		!	Get_Comp_Details

!************************************
BEGIN-PROCEDURE Get_Base_Comp_Element
!************************************
LET $RATE_CODE_CLASS	= ''
LET $FREQ_ANNUAL_FACTOR	= ''
LET #FREQ_ANNUAL_FACTOR	= 0
LET $USE_STD_HOURS	= ''
LET $FREQ_DETAILS	= ''
LET $COMP_FREQ		= ''
BEGIN-SELECT
COMP_RATECD
RT.DESCR
RATE_CODE_CLASS
COMP_BASE_PAY_SW
COMP_RATE_TYPE
COMP_PCT
RT.COMP_FREQUENCY
RT.CURRENCY_CD
	LET $BASE_COMP_RATECD		= ltrim(rtrim(&COMP_RATECD,' '),' ')
	LET $RATE_CODE_CLASS		= ltrim(rtrim(&RATE_CODE_CLASS,' '),' ')
	
	lookup RATECD_CLS_TBL $RATE_CODE_CLASS $COMP_ELEMENT_NM
	LET #COMP_ELEMENT_AMT		= &COMP.COMPRATE
	
	LET $COMP_FREQ			= ltrim(rtrim(&COMP.COMP_FREQUENCY,' '),' ')
	lookup FREQUENCY_TBL $COMP_FREQ $FREQ_DETAILS
	unstring $FREQ_DETAILS by ';' into $COMP_FREQUENCY $FREQ_ANNUAL_FACTOR $USE_STD_HOURS
	LET #FREQ_ANNUAL_FACTOR		= TO_NUMBER($FREQ_ANNUAL_FACTOR)
	
	LET $COMP_CURRENCY_CD		= ltrim(rtrim(&COMP.CURRENCY_CD,' '),' ')
FROM PS_COMP_RATECD_TBL RT
WHERE COMP_BASE_PAY_SW = 'Y'
AND RT.COMP_RATECD = $COMP_RATECD
AND RT.EFFDT =
  (SELECT MAX(RT_ED.EFFDT)
  FROM PS_COMP_RATECD_TBL RT_ED
  WHERE RT.COMP_RATECD = RT_ED.COMP_RATECD
  AND RT_ED.EFFDT     <= $SYSDATE
  )
AND RT.EFF_STATUS  = 'A'
END-SELECT

END-PROCEDURE		!	Get_Base_Comp_Element

!************************************
BEGIN-PROCEDURE Get_Allowance_Element
!************************************
BEGIN-SELECT
RTA.COMP_RATECD
RTA.DESCR
RTA.COMP_BASE_PAY_SW
RTA.COMP_RATE_TYPE
RTA.COMP_PCT
RTA.COMP_FREQUENCY
RTA.CURRENCY_CD
	
	IF #COUNT = 1
		LET $ALLOWANCE1_NM	= ltrim(rtrim(&RTA.DESCR,' '),' ')
		EVALUATE &RTA.COMP_RATE_TYPE
		
		WHEN = 'PC'
			LET #ALLOWANCE1_PER	= &COMP.COMP_PCT
		BREAK
		WHEN = 'FA'
			LET #ALLOWANCE1_AMT	= &COMP.COMPRATE
		BREAK
		WHEN-OTHER
		
		BREAK
		END-EVALUATE
		LET $ALLOWANCE1_CURR	= ltrim(rtrim(&COMP.CURRENCY_CD,' '),' ')
		LET $ALLOWANCE1_FREQ	= ltrim(rtrim(&COMP.COMP_FREQUENCY,' '),' ')
	END-IF
	IF #COUNT = 2
		LET $ALLOWANCE2_NM	= ltrim(rtrim(&RTA.DESCR,' '),' ')
		EVALUATE &RTA.COMP_RATE_TYPE
		WHEN = 'PC'
			LET #ALLOWANCE2_PER	= &COMP.COMP_PCT
		BREAK
		WHEN = 'FA'
			LET #ALLOWANCE2_AMT	= &COMP.COMPRATE
		BREAK
		WHEN-OTHER
		
		BREAK
		END-EVALUATE
		LET $ALLOWANCE2_CURR	= ltrim(rtrim(&COMP.CURRENCY_CD,' '),' ')
		LET $ALLOWANCE2_FREQ	= ltrim(rtrim(&COMP.COMP_FREQUENCY,' '),' ')
	END-IF
	IF #COUNT = 3
		LET $ALLOWANCE3_NM	= ltrim(rtrim(&RTA.DESCR,' '),' ')
		EVALUATE &RTA.COMP_RATE_TYPE
		WHEN = 'PC'
			LET #ALLOWANCE3_PER	= &COMP.COMP_PCT
		BREAK
		WHEN = 'FA'
			LET #ALLOWANCE3_AMT	= &COMP.COMPRATE
		BREAK
		WHEN-OTHER
		
		BREAK
		END-EVALUATE
		LET $ALLOWANCE3_CURR	= ltrim(rtrim(&COMP.CURRENCY_CD,' '),' ')
		LET $ALLOWANCE3_FREQ	= ltrim(rtrim(&COMP.COMP_FREQUENCY,' '),' ')
	END-IF
	IF #COUNT = 4
		LET $ALLOWANCE4_NM	= ltrim(rtrim(&RTA.DESCR,' '),' ')
		EVALUATE &RTA.COMP_RATE_TYPE
		WHEN = 'PC'
			LET #ALLOWANCE4_PER	= &COMP.COMP_PCT
		BREAK
		WHEN = 'FA'
			LET #ALLOWANCE4_AMT	= &COMP.COMPRATE
		BREAK
		WHEN-OTHER
		
		BREAK
		END-EVALUATE
		LET $ALLOWANCE4_CURR	= ltrim(rtrim(&COMP.CURRENCY_CD,' '),' ')
		LET $ALLOWANCE4_FREQ	= ltrim(rtrim(&COMP.COMP_FREQUENCY,' '),' ')
	END-IF
	IF #COUNT = 5
		LET $ALLOWANCE5_NM	= ltrim(rtrim(&RTA.DESCR,' '),' ')
		EVALUATE &RTA.COMP_RATE_TYPE
		WHEN = 'PC'
			LET #ALLOWANCE5_PER	= &COMP.COMP_PCT
		BREAK
		WHEN = 'FA'
			LET #ALLOWANCE5_AMT	= &COMP.COMPRATE
		BREAK
		WHEN-OTHER
		
		BREAK
		END-EVALUATE
		LET $ALLOWANCE5_CURR	= ltrim(rtrim(&COMP.CURRENCY_CD,' '),' ')
		LET $ALLOWANCE5_FREQ	= ltrim(rtrim(&COMP.COMP_FREQUENCY,' '),' ')
	END-IF
	LET #COUNT = #COUNT + 1
FROM PS_COMP_RATECD_TBL RTA
WHERE RTA.EFFDT =
  (SELECT MAX(RTA_ED.EFFDT)
  FROM PS_COMP_RATECD_TBL RTA_ED
  WHERE RTA.COMP_RATECD = RTA_ED.COMP_RATECD
  AND RTA_ED.EFFDT     <= $SYSDATE
  )
AND RTA.EFF_STATUS  = 'A'
AND COMP_BASE_PAY_SW <> 'Y'
AND RTA.COMP_RATECD = $COMP_RATECD
AND RTA.COMP_RATECD in ([$ALLOWANCE_PLAN])
END-SELECT

END-PROCEDURE		!	Get_Allowance_Element

!********************************
BEGIN-PROCEDURE Get_Bonus_Element
!********************************
BEGIN-SELECT
RTB.COMP_RATECD
RTB.DESCR
RTB.COMP_BASE_PAY_SW
RTB.COMP_RATE_TYPE
RTB.COMP_PCT
RTB.COMP_FREQUENCY
RTB.CURRENCY_CD
	IF &COMP.COMP_PCT <> 0
		LET #BONUS_TARGET	= &COMP.COMP_PCT
		LET $BONUS_TYPE		= 'Y'
	ELSE
		LET #BONUS_TARGET	= &COMP.COMPRATE
		LET $BONUS_TYPE		= 'N'
	END-IF
	LET $BONUS_NM		= &RTB.DESCR
	LET $BONUS_FREQ_CD	= ltrim(rtrim(&COMP.COMP_FREQUENCY,' '),' ')
	lookup FREQUENCY_TBL $BONUS_FREQ_CD $BONUS_FREQ_DETAILS
	LET #LOC_FREQ	= 0
	find ';' in $BONUS_FREQ_DETAILS 0 #LOC_FREQ
	IF #LOC_FREQ	<> -1
		LET $BONUS_FREQ	= ltrim(rtrim(substrp($BONUS_FREQ_DETAILS, 1, #LOC_FREQ),' '),' ')
	END-IF
	LET $BONUS_CURRENCY	= ltrim(rtrim(&COMP.CURRENCY_CD,' '),' ')
	
FROM PS_COMP_RATECD_TBL RTB
WHERE RTB.EFFDT =
  (SELECT MAX(RTB_ED.EFFDT)
  FROM PS_COMP_RATECD_TBL RTB_ED
  WHERE RTB.COMP_RATECD = RTB_ED.COMP_RATECD
  AND RTB_ED.EFFDT     <= $SYSDATE
  )
AND RTB.EFF_STATUS  = 'A'
AND COMP_BASE_PAY_SW <> 'Y'
AND RTB.COMP_RATECD = $COMP_RATECD
AND RTB.COMP_RATECD in ([$BONUS_PLAN])
END-SELECT

END-PROCEDURE		!	Get_Bonus_Element

!********************************
BEGIN-PROCEDURE Get_Merit_Element
!********************************
BEGIN-SELECT
RTM.COMP_RATECD
RTM.DESCR
RTM.COMP_BASE_PAY_SW
RTM.COMP_RATE_TYPE
RTM.COMP_PCT
RTM.COMP_FREQUENCY
RTM.CURRENCY_CD
	IF &COMP.COMP_PCT <> 0
		LET #MERIT_TARGET	= &COMP.COMP_PCT
		LET $MERIT_TYPE		= 'Y'
	ELSE
		LET #MERIT_TARGET	= &COMP.COMPRATE
		LET $MERIT_TYPE		= 'N'
	END-IF
	LET $MERIT_NM		= &RTM.DESCR
	LET $MERIT_FREQ_CD	= ltrim(rtrim(&COMP.COMP_FREQUENCY,' '),' ')
	lookup FREQUENCY_TBL $MERIT_FREQ_CD $MERIT_FREQ_DETAILS
	LET #LOC_FREQ	= 0
	find ';' in $MERIT_FREQ_DETAILS 0 #LOC_FREQ
	IF #LOC_FREQ	<> -1
		LET $MERIT_FREQ	= ltrim(rtrim(substrp($MERIT_FREQ_DETAILS, 1, #LOC_FREQ),' '),' ')
	END-IF
	LET $MERIT_CURRENCY	= ltrim(rtrim(&COMP.CURRENCY_CD,' '),' ')
	
FROM PS_COMP_RATECD_TBL RTM
WHERE RTM.EFFDT =
  (SELECT MAX(RTM_ED.EFFDT)
  FROM PS_COMP_RATECD_TBL RTM_ED
  WHERE RTM.COMP_RATECD = RTM_ED.COMP_RATECD
  AND RTM_ED.EFFDT     <= $SYSDATE
  )
AND RTM.EFF_STATUS  = 'A'
AND COMP_BASE_PAY_SW <> 'Y'
AND RTM.COMP_RATECD = $COMP_RATECD
AND RTM.COMP_RATECD in ([$MERIT_PLAN])
END-SELECT

END-PROCEDURE		!	Get_Merit_Element


